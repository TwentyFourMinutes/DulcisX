{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/**.csproj" ],
          "src": "../../DulcisX - master/src/DulcisX/DulcisX"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      },
	  {
		"files":[
			"guides/**.md",
			"guides/**/toc.yml"
		]
	  },
	  {
        "files": [
          "sdk/**.md",
          "sdk/**/toc.yml",
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
		  "**/samples/**"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../",
    "globalMetadataFiles": [],
	"globalMetadata": {
	    "_appTitle": "DulcisX Documentation",
		"_appFooter": "<span>Copyright Â© 2020 Twenty</span>",
		"_enableSearch": "true"
	},
    "fileMetadataFiles": [],
    "template": [
      "default",
	  "templates/light-dark-theme"
    ],
    "postProcessors": [ 
		"ExtractSearchIndex" 
	],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}